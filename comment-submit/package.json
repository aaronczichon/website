{
	"name": "comment-submit",
	"description": "Creates a comment on a blogpost triggered by a form submission",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.2",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.9.0",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"multer": "^1.4.5-lts.1"
	}
}
